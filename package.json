{
  "name": "taskmanager",
  "version": "1.0.0",
  "description": "Backend for a task manager app made with nodeJs",
  "main": "index.js",
  "scripts": {
    "start": "npm run db:up && nodemon index.js",
    "db:up": "npm run db:migrate && npm run db:seed",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:down": "npx sequelize-cli db:seed:undo:all && npx sequelize-cli db:migrate:undo:all",
    "db:restart": "npm run db:down && npm run db:up",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EzequielOmar/task-manager.git"
  },
  "keywords": [
    "nodeJs",
    "expressJs",
    "jwt",
    "passport",
    "sequelize",
    "postgresql",
    "nodedock"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EzequielOmar/task-manager/issues"
  },
  "homepage": "https://github.com/EzequielOmar/task-manager#readme",
  "devDependencies": {
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.5.2"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "pg": "^8.8.0",
    "sequelize": "^6.26.0"
  }
}
